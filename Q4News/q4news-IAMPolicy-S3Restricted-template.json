{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
            "s3:CreateBucket",
            "s3:DeleteBucket",
            "s3:ListBucket",
            "s3:ListBucketVersions",
            "s3:ListAllMyBuckets",
            "s3:ListBucketMultipartUploads"
          ],
            "Resource":[
              "arn:aws:s3:us-east-1::*"
            ]
        }
    ]
}